{
  "name": "happydb-server",
  "version": "1.0.0",
  "description": "A server for serving up happy DB stuff!",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "fmt": "prettier --write '**/*.{jsx,js}'"
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always",
    "jsxSingleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "yarn fmt",
      "git add"
    ]
  },
  "author": "Joshua Johnson, Joseph Wisor, Adriana Holley, Madison Martin",
  "license": "ISC",
  "dependencies": {
    "csv-parser": "^2.2.0",
    "csv-write-stream": "^2.0.0",
    "express": "^4.16.4",
    "husky": "^2.2.0",
    "json2csv": "^4.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "tedious": "^5.0.3"
  }
}
